pipeline {
    agent any

  	stages {
	  	stage ("Clone from GitHub"){
	  	    agent { label 'main' }
			    steps{
					echo "cloning"
					git branch: 'main',
						credentialsId: 'c1f7b986-264a-46f9-a66d-424ec1168ed2',
						url: 'https://github.com/yawani/safozi.git'
			    } 
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
